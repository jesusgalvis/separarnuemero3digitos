#include <stdio.h>
#include <stdlib.h>

int main(){
	
	int num,dig,v1,v2,v3;
	
	
	printf("ingrese un numero de 3 cifras\n");
	scanf("%d",&num);
	
	dig=num%10;
	v1=dig;
	num=num/10;
	dig=num%10;
	v2=dig;
	num=num/10;
	dig=num%10;
	v3=dig;
	
	printf("el numero separado es %d %d %d",v1,v2,v3);
	
	
	
	
	return 0;
}
